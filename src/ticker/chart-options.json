{
  "animation": true,
  "chart": {
    "height": 400,
    "animation": true,
    "reflow": false,
    "zoomType": "x",
    "style": {
      "fontFamily": [
        "Roboto",
        "Helvetica Neue",
        "sans-serif"
      ]
    }
  },
  "legend": {
    "enabled": false
  },
  "credits": {
    "enabled": false
  },
  "rangeSelector": {
    "buttons": [
      {
        "type": "minute",
        "count": 5,
        "text": "m"
      },
      {
        "type": "hour",
        "count": 1,
        "text": "h"
      },
      {
        "type": "hour",
        "count": 8,
        "text": "d"
      },
      {
        "type": "day",
        "count": 5,
        "text": "w"
      },
      {
        "type": "all",
        "count": 1,
        "text": "all"
      }
    ],
    "inputEnabled": false
  },
  "yAxis": {
    "title": {
      "text": "Portfolio Value ($)"
    }
  },
  "tooltip": {
    "valueDecimals": 2,
    "valuePrefix": "$"
  },
  "series": [],
  "title": {
    "text": "My Portfolio"
  },
  "navigator": {
    "enabled": true,
    "adaptToUpdatedData": true
  },
  "enableMouseTracking": false
}